"amixer set Master 5%+"
	m:0x0 + c:123
	XF86AudioRaiseVolume

"amixer set Master 5%-"
	m:0x0 + c:122
	XF86AudioLowerVolume

"amixer set Master toggle"
	m:0x0 + c:121
	XF86AudioMute

"brightnessctl s +2%"
	m:0x0 + c:233
	XF86MonBrightnessUp

"brightnessctl s 2%-"
	m:0x0 + c:232
	XF86MonBrightnessDown
